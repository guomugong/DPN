net: "dpn_train.prototxt"

test_iter: 20 # DRIVE
test_interval: 1000
test_initialization: false

display: 20000

base_lr: 1e-3
lr_policy: "fixed"
solver_type:ADAM
#lr_policy: "step"
#stepsize: 30000
#gamma: 0.1

momentum: 0.9
weight_decay: 0.0005


snapshot: 2000
snapshot_prefix: "snapshot/dpn_drive"

max_iter: 100000
solver_mode: GPU
